<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ScrollView
        android:id="@+id/scrollView_consultas_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="0dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/tv_consulta_correo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginTop="15dp"
                android:text="Correo electrónico:" />

            <EditText
                android:id="@+id/et_consulta_correo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignRight="@id/tv_consulta_correo"
                android:hint="correo electrónico"
                android:inputType="textEmailAddress" />


            <TextView
                android:id="@+id/tv_consulta_password"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/tv_consulta_correo"
                android:layout_marginTop="25dp"
                android:text="Contraseña:" />

            <EditText
                android:id="@+id/et_consulta_password"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/et_consulta_correo"
                android:hint="contraseña"
                android:inputType="textPassword" />

            <TextView
                android:id="@+id/tv_consulta_nombre"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/tv_consulta_password"
                android:layout_marginTop="25dp"
                android:text="Nombre:" />

            <EditText
                android:id="@+id/et_consulta_nombre"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/et_consulta_password"
                android:hint="nombre"
                android:inputType="textCapWords"
                android:maxLength="100" />

            <TextView
                android:id="@+id/tv_consulta_apellidos"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/tv_consulta_nombre"
                android:layout_marginTop="25dp"
                android:text="Apellidos:" />

            <EditText
                android:id="@+id/et_consulta_apellidos"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/et_consulta_nombre"
                android:hint="apellidos"
                android:inputType="textCapWords"
                android:maxLength="100" />

            <LinearLayout
                android:id="@+id/ll0"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/et_consulta_apellidos"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_consulta_telefono_home"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Tfno. fijo:" />

                <EditText
                    android:id="@+id/et_consulta_telefono_home"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:inputType="text|phone"
                    android:maxLength="20"
                    android:hint="teléfono fijo"
                    android:layout_marginLeft="5dp"/>

                <TextView
                    android:id="@+id/tv_consulta_telefono_mobile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Tfno. móvil:"
                    android:layout_marginLeft="5dp"/>

                <EditText
                    android:id="@+id/et_consulta_telefono_mobile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="teléfono móvil"
                    android:inputType="text|phone"
                    android:maxLength="20" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/ll1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@id/ll0"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_consulta_provincia"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:text="Provincia:" />

                <Spinner
                    android:id="@+id/sp_consulta_provincia"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"></Spinner>

                <TextView
                    android:id="@+id/tv_consulta_municipio"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Municipio:" />

                <Spinner
                    android:id="@+id/sp_consulta_municipio"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"></Spinner>

            </LinearLayout>

            <TextView
                android:id="@+id/tv_consulta_capital"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/ll1"
                android:layout_marginTop="20dp"
                android:text="Capital:" />

            <EditText
                android:id="@+id/et_consulta_capital"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/ll1"
                android:hint="capital"
                android:inputType="text"
                android:maxLength="100" />


            <TextView
                android:id="@+id/tv_consulta_capital_observaciones"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/et_consulta_capital"
                android:layout_marginTop="15dp"
                android:text="Capital Observaciones:" />

            <EditText
                android:id="@+id/et_consulta_capital_observaciones"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/tv_consulta_capital"
                android:hint="capital observaciones"
                android:inputType="text"
                android:maxLength="255" />


            <TextView
                android:id="@+id/tv_consulta_cerrada"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/et_consulta_capital_observaciones"
                android:layout_marginTop="15dp"
                android:text="Cerrada:" />

            <EditText
                android:id="@+id/et_consulta_cerrada"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/tv_consulta_capital_observaciones"
                android:hint="cerrada"
                android:inputType="number" />

            <TextView
                android:id="@+id/tv_consulta_cuando_empezar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/tv_consulta_cerrada"
                android:layout_marginTop="20dp"
                android:text="Cuando empezar:" />

            <EditText
                android:id="@+id/et_consulta_cuando_empezar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/et_consulta_cerrada"
                android:hint="cuando empezar"
                android:inputType="text"
                android:maxLength="100" />

            <TextView
                android:id="@+id/tv_consulta_dispone_contacto"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/tv_consulta_cuando_empezar"
                android:layout_marginTop="20dp"
                android:text="Dispone contacto:" />

            <EditText
                android:id="@+id/et_consulta_dispone_contacto"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/et_consulta_cuando_empezar"
                android:hint="dispone contacto"
                android:inputType="text"
                android:maxLength="100" />

            <TextView
                android:id="@+id/tv_consulta_dispone_local"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/tv_consulta_dispone_contacto"
                android:layout_marginTop="25dp"
                android:text="Dispone local:" />

            <EditText
                android:id="@+id/et_consulta_dispone_local"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/et_consulta_dispone_contacto"
                android:hint="dispone local"
                android:inputType="number" />

            <TextView
                android:id="@+id/tv_consulta_empresa"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/tv_consulta_dispone_local"
                android:layout_marginTop="25dp"
                android:text="Empresa:" />

            <EditText
                android:id="@+id/et_consulta_empresa"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/et_consulta_dispone_local"
                android:hint="empresa"
                android:inputType="text"
                android:maxLength="255" />


            <TextView
                android:id="@+id/tv_consulta_negocio"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/tv_consulta_empresa"
                android:layout_marginTop="25dp"
                android:text="Negocio:" />

            <EditText
                android:id="@+id/et_consulta_negocio"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/et_consulta_empresa"
                android:hint="negocio"
                android:inputType="text"
                android:maxLength="255" />

            <TextView
                android:id="@+id/tv_consulta_negocio_anterior"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/tv_consulta_negocio"
                android:layout_marginTop="25dp"
                android:text="Negocio anterior:" />

            <EditText
                android:id="@+id/et_consulta_negocio_anterior"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/et_consulta_negocio"
                android:hint="negocio anterior"
                android:inputType="number" />

            <TextView
                android:id="@+id/tv_consulta_perfil_franquicia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/tv_consulta_negocio_anterior"
                android:layout_marginTop="25dp"
                android:text="Perfil franquicia:" />

            <EditText
                android:id="@+id/et_consulta_perfil_franquicia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/et_consulta_negocio_anterior"
                android:hint="perfil franquicia"
                android:inputType="text"
                android:maxLength="100" />

            <TextView
                android:id="@+id/tv_consulta_perfil_profesional"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/tv_consulta_perfil_franquicia"
                android:layout_marginTop="25dp"
                android:text="Perfil profesional:" />

            <EditText
                android:id="@+id/et_consulta_perfil_profesional"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/et_consulta_perfil_franquicia"
                android:hint="perfil profesional"
                android:inputType="text" />

            <TextView
                android:id="@+id/tv_consulta_recursos_propios"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/tv_consulta_perfil_profesional"
                android:layout_marginTop="25dp"
                android:text="Recursos propios:" />

            <EditText
                android:id="@+id/et_consulta_recursos_propios"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/et_consulta_perfil_profesional"
                android:hint="recursos propios"
                android:inputType="text"
                android:maxLength="100" />

            <TextView
                android:id="@+id/tv_consulta_situacion_profesional"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/tv_consulta_recursos_propios"
                android:layout_marginTop="30dp"
                android:text="Situacion profesional:" />

            <EditText
                android:id="@+id/et_consulta_situacion_profesional"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/et_consulta_recursos_propios"
                android:hint="situacion profesional"
                android:inputType="text"
                android:maxLength="100" />

            <Button
                android:id="@+id/boton_consulta_user"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_below="@+id/et_consulta_situacion_profesional"
                android:onClick="consultaUsuario2"
                android:text="ACEPTAR" />


        </RelativeLayout>
    </ScrollView>

</RelativeLayout>
